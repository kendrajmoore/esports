<!-- events-index -->
  {{!-- <div class="row mt-4"> --}}
    <div class="row">
      <div class="col-md-6">
           <h1>Events</h1>
          <a href="/events/new">New Event</a>
          {{#each events}}
          <div class="card mt-3">
            <img class="card-img-top" src="{{this.imgUrl}}" alt="Card image cap">
            <div class="card-body">
              <h5 class="card-title">Event: {{this.eventName}}</h5>
              <p class="card-text">Details: {{this.desc}}</p>
              <p class="card-text">Game: {{this.game}}</p>
              <p class="card-text">Location: {{this.location}}</p>
              <div class="text-right">
                <a href="#" class="btn btn-info">Attend</a>
                <a href="/events/{{this.id}}" class="btn btn-primary">View</a>
                <a href="/events/{{this.id}}/rsvps/new" class="btn btn-info">Rsvp</a>

              </div>
            </div>
          </div>
        {{/each}}
      </div>
    </div>
  {{!-- </div> --}}